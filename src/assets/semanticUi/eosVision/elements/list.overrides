/*******************************
    User Variable Overrides
*******************************/

.ui.list.list-view {
  border: @dividedBorder;
  >.item {
    display: flex;
    align-items: center;
    padding: 0;
    & when (@VisionDark = false) { 
    background-color: @white-bg; 
    } 
    & when (@VisionDark = true) { 
      background: @eosGrey13-bg; 
    }
  }
}
