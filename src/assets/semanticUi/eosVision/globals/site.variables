@eosGrey10-bg: @eosGrey10;
@eosGrey11-bg: @eosGrey11;

@selectedBackgroudColor: @eosBlue9;
@selectedBorderColor: @blue;


/* Negative */
@negativeColor           : @red;
@negativeBackgroundColor : @white-bg;
@negativeBorderColor     : @red;
@negativeHeaderColor     : #912D2B;
@negativeTextColor       : @textColor;

/*-------------------
    Scroll Bars
--------------------*/

@customScrollbarWidth: 9px;

@trackBackground: fade(@eosGrey9-1-bg, 22%);

@thumbBorderRadius: 3px;
@thumbBackground: @eosGrey9-1-bg;

/*-------------------
    inputs disabled
--------------------*/
  @inputDisabledBackground: @eosGrey11-bg;
  @inputDisabledcolor: @eosGrey4-text;
  @inputDisabledBorder: @borderColor;


/*-------------------
    Focused Input
--------------------*/

/* Used on inputs, textarea etc */
@focusedFormBorderColor: @eosBlue4;

@loaderLineWidth: 0.2rem;
@loaderFillColor         : rgba(0, 0, 0, 0.15);
@loaderLineColor         : @blue;


/*-------------------
        Sizes
--------------------*/


@miniSize        : (12 / 14);

@relativeBorderRadius: @relative4px;
@absoluteBorderRadius: @4px;


/*-------------------
       Custom EOS Variables
--------------------*/

@eos-header-height: 52px;
@eos-subHeader-height: 52px;
@eos-header-zIndex: 1040;
@eos-subHeader-zIndex: 1030;

@eos-sidebar-zIndex: 102;
@eos-upgrade-alert-zIndex: 103;

@eos-notificationsMessagesList-zIndex: 1060;
@eos-lasso-zIndex: 1070;

@eos-basemapPopup-zIndex: 104;

/*-------------------
       Custom box-shadow
--------------------*/

@eosBoxShadow-1: 0 1px 3px rgba(0, 0, 0, 0.12), 0 2px 6px rgba(0, 0, 0, 0.24);
@eosBoxShadow-2: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 8px rgba(0, 0, 0, 0.23);
@eosBoxShadow-3: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 10px rgba(0, 0, 0, 0.23);
@eosBoxShadow-4: 0 14px 28px rgba(0, 0, 0, 0.25), 0 10px 15px rgba(0, 0, 0, 0.22);
@eosBoxShadow-5: 0 19px 38px rgba(0, 0, 0, 0.30), 0 15px 20px rgba(0, 0, 0, 0.22);

@import "../../../fontIcons/variables.less";
